<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:HundeKennel"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
    x:Class="HundeKennel.MainWindow"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Grid>

        <syncfusion:SfLinearProgressBar x:Name="ProgressBar" HorizontalAlignment="Center" Height="20" VerticalAlignment="Center" Width="200"/>
        <Button Content="Button" HorizontalAlignment="Center" Margin="0,170,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>
</Window>
